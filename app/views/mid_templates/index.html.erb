<h1>Listing mid_templates</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mid_templates.each do |mid_template| %>
      <tr>
        <td><%= link_to 'Show', mid_template %></td>
        <td><%= link_to 'Edit', edit_mid_template_path(mid_template) %></td>
        <td><%= link_to 'Destroy', mid_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mid template', new_mid_template_path %>
